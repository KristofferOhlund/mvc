
<div class="adventure">
    <div class="background">
        <img src=" {{ asset('build/images/adventure/' ~ img) }} " alt="graveyard">
    </div>
    <div class="content">
        {% for item in items %}
            <p>{{ item.getName() }}</p>
        {% endfor %}
        {% for room in rooms %}
            <p>{{ room.getName() }}</p>
        {% endfor %}
    </div>
</div>
